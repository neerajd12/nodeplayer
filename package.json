{
  "name": "NodePlayer",
  "description": "Connected music player",
  "version": "0.1.0",
  "author": "Neeraj Dixit",
  "main": "main.js",
  "build": {
    "appId": "com.neerajdixit.nodeplayer",
    "linux": {
      "category": "Audio;AudioVideo;",
      "icon": "build/icon.png"
    },
    "win": {
      "iconUrl": "https://github.com/neerajdixit/nodeplayer/tree/master/img/icon.png.ico"
    }
  },
  "scripts": {
